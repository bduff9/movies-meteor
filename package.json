{
  "name": "movies",
  "version": "0.0.1",
  "description": "A media tracker using Meteor-React, Apollo, and MySQL",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/movies.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "initApp": "meteor npm i && meteor npm prune",
    "prestart": "meteor npm run initApp",
    "predebug": "meteor npm run initApp",
    "start": "meteor run --settings settings.json",
    "debug": "meteor run --inspect --settings settings.json",
    "debug-brk": "meteor run --inspect-brk --settings settings.json",
    "debug-old": "meteor debug --settings settings.json",
    "lint": "eslint --fix \"**/*.js\" \"**/*.jsx\"",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:meteor": "meteor test --driver-package=practicalmeteor:mocha --port=3004",
    "precommit": "lint-staged && npm test"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.3.4",
    "@fortawesome/fontawesome-pro": "5.7.2",
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-brands-svg-icons": "5.7.2",
    "@fortawesome/free-regular-svg-icons": "5.7.2",
    "@fortawesome/pro-light-svg-icons": "5.7.2",
    "@fortawesome/pro-regular-svg-icons": "5.7.2",
    "@fortawesome/pro-solid-svg-icons": "5.7.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "apollo-boost": "0.3.1",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-link-http": "1.5.11",
    "apollo-server-express": "2.4.8",
    "aws-amplify": "1.1.19",
    "aws-amplify-react": "2.3.0",
    "bloomer": "0.6.5",
    "bulma_scss": "0.7.2-rc.0",
    "bulmaswatch": "0.7.2",
    "date-fns": "2.0.0-alpha.27",
    "express": "4.16.4",
    "formik": "1.5.1",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.4",
    "immutable": "4.0.0-rc.12",
    "jsonwebtoken": "8.5.0",
    "jwk-to-pem": "2.0.1",
    "meteor-node-stubs": "0.4.1",
    "mysql2": "1.6.5",
    "react": "16.8.3",
    "react-apollo": "2.5.1",
    "react-dom": "16.8.3",
    "react-router-dom": "4.3.1",
    "sequelize": "4.43.0",
    "styled-components": "4.1.3",
    "yup": "0.26.10"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@types/meteor": "1.4.27",
    "@types/react": "16.8.6",
    "@types/react-fontawesome": "1.6.4",
    "@types/react-router-dom": "4.3.1",
    "@types/sequelize": "4.27.38",
    "autoprefixer": "9.6.0",
    "babel-eslint": "10.0.1",
    "coveralls": "3.0.3",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "eslint": "5.15.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.4.0",
    "eslint-plugin-you-dont-need-momentjs": "1.4.0",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "jest-transform-css": "2.0.0",
    "lint-staged": "8.1.5",
    "postcss": "<7.0.0",
    "postcss-load-config": "<2.0.0",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "ie >= 9",
          "and_chr >= 2.3"
        ]
      }
    }
  }
}
