{
  "name": "movies",
  "version": "0.0.1",
  "description": "A media tracker using Meteor-React, Apollo, and MySQL",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/movies.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "initApp": "meteor npm prune && meteor npm i",
    "prestart": "meteor npm run initApp",
    "predebug": "meteor npm run initApp",
    "start": "meteor run --settings settings.json",
    "debug": "meteor run --inspect --settings settings.json",
    "debug-brk": "meteor run --inspect-brk --settings settings.json",
    "debug-old": "meteor debug --settings settings.json",
    "test": "meteor test --driver-package=practicalmeteor:mocha --port=3004"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.49",
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-brands": "5.0.13",
    "@fortawesome/fontawesome-pro-light": "5.0.13",
    "@fortawesome/fontawesome-pro-regular": "5.0.13",
    "@fortawesome/fontawesome-pro-solid": "5.0.13",
    "@fortawesome/react-fontawesome": "0.0.20",
    "apollo-cache-inmemory": "1.2.2",
    "apollo-client": "2.3.2",
    "apollo-link-http": "1.5.4",
    "bloomer": "0.6.3",
    "body-parser": "1.18.3",
    "bulma_scss": "0.7.1-beta.2",
    "bulmaswatch": "0.6.2",
    "express": "4.16.3",
    "graphql": "0.13.2",
    "graphql-server-express": "1.3.6",
    "graphql-tag": "2.9.2",
    "graphql-tools": "3.0.2",
    "immutable": "3.8.2",
    "meteor-node-stubs": "0.4.1",
    "moment": "2.22.2",
    "mysql2": "1.5.3",
    "prop-types": "15.6.1",
    "react": "16.4.2",
    "react-apollo": "2.1.4",
    "react-dom": "16.4.0",
    "sequelize": "4.37.10",
    "styled-components": "3.3.2"
  },
  "devDependencies": {
    "@types/meteor": "1.4.15",
    "@types/react-fontawesome": "1.6.3",
    "autoprefixer": "8.6.0",
    "babel-eslint": "8.2.3",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-react": "7.9.1",
    "jshint": "2.9.5",
    "postcss": "6.0.22",
    "postcss-load-config": "1.2.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "ie >= 9",
          "and_chr >= 2.3"
        ]
      }
    }
  }
}
