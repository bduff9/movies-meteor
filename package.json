{
  "name": "movies",
  "version": "0.0.1",
  "description": "A media tracker using Meteor-React, Apollo, and MySQL",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/movies.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "initApp": "meteor npm prune && meteor npm i",
    "prestart": "meteor npm run initApp",
    "predebug": "meteor npm run initApp",
    "start": "meteor run --settings settings.json",
    "debug": "meteor run --inspect --settings settings.json",
    "debug-brk": "meteor run --inspect-brk --settings settings.json",
    "debug-old": "meteor debug --settings settings.json",
    "lint": "eslint \"**/*.js\" \"**/*.jsx\"",
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test_meteor": "meteor test --driver-package=practicalmeteor:mocha --port=3004"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "@fortawesome/fontawesome-pro": "5.3.1",
    "@fortawesome/fontawesome-svg-core": "1.2.6",
    "@fortawesome/free-brands-svg-icons": "5.4.1",
    "@fortawesome/free-regular-svg-icons": "5.4.1",
    "@fortawesome/pro-light-svg-icons": "5.3.1",
    "@fortawesome/pro-regular-svg-icons": "5.3.1",
    "@fortawesome/pro-solid-svg-icons": "5.3.1",
    "@fortawesome/react-fontawesome": "0.1.3",
    "apollo-cache-inmemory": "1.2.10",
    "apollo-client": "2.4.2",
    "apollo-link-http": "1.5.4",
    "bloomer": "0.6.3",
    "body-parser": "1.18.3",
    "bulma_scss": "0.7.1-beta.2",
    "bulmaswatch": "0.7.1",
    "express": "4.16.3",
    "graphql": "0.13.2",
    "graphql-server-express": "1.4.0",
    "graphql-tag": "2.9.2",
    "graphql-tools": "3.1.1",
    "immutable": "3.8.2",
    "meteor-node-stubs": "0.4.1",
    "moment": "2.22.2",
    "mysql2": "1.6.1",
    "prop-types": "15.6.2",
    "react": "16.5.0",
    "react-apollo": "2.1.11",
    "react-dom": "16.5.0",
    "sequelize": "4.38.1",
    "styled-components": "3.4.6"
  },
  "devDependencies": {
    "@types/meteor": "1.4.21",
    "@types/react-fontawesome": "1.6.4",
    "autoprefixer": "9.1.5",
    "babel-eslint": "9.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "coveralls": "3.0.2",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "jest": "23.6.0",
    "jshint": "2.9.6",
    "postcss": "6.0.23",
    "postcss-load-config": "1.2.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "ie >= 9",
          "and_chr >= 2.3"
        ]
      }
    }
  }
}
