{
  "name": "movies",
  "version": "0.0.1",
  "description": "A media tracker using Meteor-React, Apollo, and MySQL",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/movies.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "initApp": "meteor npm i && meteor npm prune",
    "prestart": "meteor npm run initApp",
    "predebug": "meteor npm run initApp",
    "start": "meteor run --settings settings.json",
    "debug": "meteor run --inspect --settings settings.json",
    "debug-brk": "meteor run --inspect-brk --settings settings.json",
    "debug-old": "meteor debug --settings settings.json",
    "lint": "eslint --fix \"**/*.js\" \"**/*.jsx\"",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:meteor": "meteor test --driver-package=practicalmeteor:mocha --port=3004",
    "precommit": "lint-staged && npm test"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.2.0",
    "@fortawesome/fontawesome-pro": "5.6.3",
    "@fortawesome/fontawesome-svg-core": "1.2.12",
    "@fortawesome/free-brands-svg-icons": "5.6.3",
    "@fortawesome/free-regular-svg-icons": "5.6.3",
    "@fortawesome/pro-light-svg-icons": "5.6.3",
    "@fortawesome/pro-regular-svg-icons": "5.6.3",
    "@fortawesome/pro-solid-svg-icons": "5.6.3",
    "@fortawesome/react-fontawesome": "0.1.4",
    "apollo-boost": "0.1.24",
    "apollo-cache-inmemory": "1.4.0",
    "apollo-link-http": "1.5.9",
    "apollo-server-express": "2.3.1",
    "bloomer": "0.6.5",
    "bulma_scss": "0.7.2-rc.0",
    "bulmaswatch": "0.7.2",
    "date-fns": "2.0.0-alpha.27",
    "express": "4.16.4",
    "formik": "1.4.2",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.3",
    "immutable": "4.0.0-rc.12",
    "meteor-node-stubs": "0.4.1",
    "mysql2": "1.6.4",
    "react": "16.7.0",
    "react-apollo": "2.3.3",
    "react-dom": "16.7.0",
    "react-router-dom": "4.3.1",
    "sequelize": "4.42.0",
    "styled-components": "4.1.3",
    "yup": "0.26.7"
  },
  "devDependencies": {
    "@types/meteor": "1.4.24",
    "@types/react": "16.7.20",
    "@types/react-fontawesome": "1.6.4",
    "@types/react-router-dom": "4.3.1",
    "@types/sequelize": "4.27.34",
    "autoprefixer": "9.4.5",
    "babel-eslint": "10.0.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "coveralls": "3.0.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.13.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-you-dont-need-momentjs": "1.4.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "postcss": "^6.0.23",
    "postcss-load-config": "^1.2.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "ie >= 9",
          "and_chr >= 2.3"
        ]
      }
    }
  }
}
